    void FloydWarshall() {
        int N = graph.size(); //так показываем количество вершин
        for (int k = 0; k < N; ++k) { // перебирает промежуточные вершины
            for (int i = 0; i < N; ++i) { //перебирает начальные вершины
                for (int j = 0; j < N; ++j) { //конечные вершины
                    if (graph[i][k] != INF && graph[k][j] != INF && graph[i][k] + graph[k][j] < graph[i][j]) { //является ли путь от вершины i до вершины j короче, если мы используем промежуточную вершину k
                        graph[i][j] = graph[i][k] + graph[k][j]; //обновляется
                    }
                }
            }
        }
    }
